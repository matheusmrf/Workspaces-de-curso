package com.ti2cc;

public class Monitores {
	private int id;
	private String nome;
	private String disciplina;
	private char turno;
	
	public Monitores() {
		this.id = -1;
		this.nome = "";
		this.disciplina = "";
		this.turno = "*";
	}

	public Monitores(int id, String nome, String disciplina, char turno) {
		this.id = id;
		this.nome = nome;
		this.disciplina = disciplina;
		this.turno = turno;
	}
	
}
